{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": [
    "syd1"
  ],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://globaltravelreport.com",
    "DISABLE_MIDDLEWARE": "true"
  },
  "crons": [
    {
      "path": "/api/cron/dailyAutoPublisher",
      "schedule": "0 0 * * *"
    }
  ]
}